                   url: "http://127.0.0.1:8000/comments-status",
                   method: "get",
                   data: {
                       commentid,
                       text
                   },
                   success: function(res) {
                       if (res == "success") {
                           if (text == "Pending") {
                               $t.text('Fixed');
                               $t.removeClass('btn btn-primary').addClass('btn btn-success');
                               $("#comment_modal_id").modal('show');
                           } else {
                               $t.text('Pending');
                               $t.removeClass('btn btn-success').addClass('btn btn-primary');
                               $("#comment_modal_id").modal('show');
                           }
   
                       } else {
                           $t.text(text);
                       }
                   }
               });
   
           });
   
   
   });
   
   
